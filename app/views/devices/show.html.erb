<%- model_class = Device -%>
<script>
    $('.devices').addClass('selected');
</script>
<!-- start of .userinfo-navsections -->
<div class="userinfo-navsections" id="headerDiv">
  <!-- start of .userinfo -->
  <ul class="userinfo"><li><%= @device.device_type %></li></ul>
  <!-- end of userinfo -->


  <ul class="menuItems">
    <li><a href="javascript:void(0)" class="about activeMenu"><span class="icon"></span>About</a></li>
    <li><a href="javascript:void(0)" class="settings"><span class="icon"></span>Settings</a></li>
    <li><a href="javascript:void(0)" class="apps"><span class="icon"></span>Apps</a></li>
    <li><a href="javascript:void(0)" class="activity"><span class="icon"></span>Activity</a></li>
  </ul>

</div>
<!-- end of .userinfo-navsections -->


<div class="contentdiv-inner">
  <!-- start of .collapseButtonsDiv -->
  <div class="collapseButtonsDiv">
    <a href="javascript:void(0)" class="expandAll" id="expandAll">Expand All</a><a href="javascript:void(0)" class="collapseAll" id="collapseAll">Collapse All</a>
  </div>
  <!-- end of .collapseButtonsDiv -->

  <!-- start of .accordion-tabs-section -->
  <div class="accordion-tabs-section">

